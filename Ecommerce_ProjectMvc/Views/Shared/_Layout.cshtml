                        </div><!-- /.widget -->
                    </div><!-- /.col-md-3 -->
                    <div class="col-sm-6 col-md-3">
                        <div class="widget widget-link link-login">
                            <ul>
                                <li><a href="@Url.Action("Login","Register")">Login </a></li>
                                <li><a href="@Url.Action("Signin","Register")">Register</a></li>
                                <li><a href="@Url.Action("Index","Home1")">Wishlist items</a></li>
                                <li><a href="@Url.Action("Index","Home1")">Your checkout</a></li>
                            </ul>
                        </div><!-- /.widget -->
                    </div><!-- /.col-md-3 -->
                    <div class="col-sm-6 col-md-3">
