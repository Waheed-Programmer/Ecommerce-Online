                                <li><a href="@Url.Action("Faq","Home1")">FAQs</a></li>
                                <li><a href="@Url.Action("Terms","Home1")">Term of service</a></li>
                                <li><a href="@Url.Action("Privacy","Home1")">Privacy Policy</a></li>
                                <li><a href="@Url.Action("Disclamer","Home1")">Desclaimer</a></li>
                            </ul>
                        </div><!-- /.widget -->
                    </div><!-- /.col-md-3 -->
                    <div class="col-sm-6 col-md-3">
                        <div class="widget widget-brand">
                            <div class="logo logo-footer">

                              
                                <a href="@Url.Action("Index","Home1")"><img src="~/Content/Blog Images/logo3.jpg" alt="image" width="107" height="24"></a>
                            </div>
                            <ul class="flat-contact">
                                <li class="address">Islamabad Pakistan</li>
                                <li class="phone">+92 302 07680 17</li>
                                <li class="email">waheedarshad239@gmail.com</li>
                            </ul><!-- /.flat-contact -->
                        </div><!-- /.widget -->
                    </div><!-- /.col-md-3 -->
                </div><!-- /.row -->
