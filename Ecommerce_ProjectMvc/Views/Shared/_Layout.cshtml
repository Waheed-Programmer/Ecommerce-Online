            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <p class="copyright text-center">Copyright 2020 <a href="@Url.Action("Index","Home1")"><b>ClothZilla.</b></a></p>
                    </div>
                </div>
            </div>
