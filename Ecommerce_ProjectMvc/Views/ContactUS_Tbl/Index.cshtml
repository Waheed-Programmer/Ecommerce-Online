@model IEnumerable<Ecommerce_ProjectMvc.Models.ContactUS_Tbl>

@{
    ViewBag.Title = "All Information > Dashboard";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="card shadow mb-4">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">All Contact Information</h6>
    </div>
    <div class="card-body">
        <div class="table-responsive">

            <table class="table table-striped table-hover" id="dataTable" width="100%" cellspacing="0">
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.Name)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Email)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Subject)
                    </th>
                    <th>
                        Message
                        @*@Html.DisplayNameFor(model => model.SMS)*@
                    </th>
                    <th></th>
                </tr>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Name)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Email)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Subject)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.SMS)
                        </td>
                        <td>
                           
                            @*@Html.ActionLink("Delete", "Delete", new { id = item.ContactId }, new { @class = "btn btn-danger" })*@
                        </td>
                    </tr>
                }

            </table>
        </div>
    </div>
</div>